{
  "version": "1.0",
  "type": "container",
  "meta": {
    "trigger": "webhook"
  },
  "ops": [
    {
      "type": "container/run",
      "id": "mastrabolt-agent",
      "args": {
        "image": "docker.io/seyman101/agent-challenge:latest",
        "expose": 8080,
        "gpu": false,
        "env": {
          "NODE_ENV": "production",
          "RUNNING_IN_DOCKER": "true",
          "VITE_LOG_LEVEL": "info",
          "PORT": "8080"
        }
      }
    }
  ]
}
